{
  "name": "roamalto",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "test": "node --test --loader ./test/ts-loader.mjs src/lib/__tests__/*.test.ts",
    "deploy:vercel": "vercel --prod --confirm",
    "db:seed": "tsx db/seed.ts",
    "dev:db": "sh -c \"docker start roamalto-postgres 2>/dev/null || docker run -d --name roamalto-postgres -e POSTGRES_PASSWORD=pass -e POSTGRES_USER=roamalto -e POSTGRES_DB=roamalto -p 5432:5432 postgres:15\"",
    "dev:db:stop": "sh -c \"docker stop roamalto-postgres 2>/dev/null && docker rm roamalto-postgres 2>/dev/null || true\""
  },
  "dependencies": {
    "@fast-csv/format": "^5.0.0",
    "@upstash/ratelimit": "^1.1.4",
    "@upstash/redis": "^1.30.1",
    "kysely": "^0.27.4",
    "next": "16.0.1",
    "next-auth": "^4.24.7",
    "nodemailer": "^7.0.7",
    "pg": "^8.13.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.15",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "tailwindcss": "^4",
    "tsx": "^4.19.1",
    "typescript": "^5",
    "vercel": "^48.7.1"
  }
}
